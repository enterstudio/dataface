{
  "name": "dataface",
  "description": "Build and manage data with a spreadsheet-like interface",
  "author": "timwis <tim@timwis.com>",
  "private": true,
  "scripts": {
    "start": "budo src/main.js:dist/build.js --live --pushstate -- -t localenvify",
    "build": "NODE_ENV=production browserify -t localenvify src/main.js | uglifyjs -c warnings=false -m > dist/build.js"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "bulma": "^0.4.2",
    "lodash": "^4.17.4",
    "shortid": "^2.2.8",
    "url-join": "^2.0.2",
    "vue": "^2.0.1",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "budo": "^10.0.3",
    "localenvify": "^1.0.1",
    "uglify-js": "^2.5.0",
    "vueify": "^9.1.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  }
}
